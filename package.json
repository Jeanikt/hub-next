{
  "name": "hub-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:apply-missing": "prisma db execute --file prisma/apply-missing-postgres.sql --schema prisma/schema.prisma",
    "db:add-cpf": "prisma db execute --file prisma/add-cpf-columns.sql --schema prisma/schema.prisma",
    "db:create-auth-tables": "prisma db execute --file prisma/create-auth-tables.sql --schema prisma/schema.prisma",
    "db:reset": "prisma db push --force-reset --accept-data-loss"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.19.2",
    "@upstash/redis": "^1.34.3",
    "@vercel/analytics": "^1.6.1",
    "bcryptjs": "^3.0.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "20.19.33",
    "@types/react": "19.2.14",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^26.0.0",
    "prisma": "^6.19.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^3.0.5"
  }
}
